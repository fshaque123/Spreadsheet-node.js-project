    </div>

    <div>
        <table class="mySheetList">
            <tr>
                <th>Sheet's name</th>
                <th>Creator</th>
                <th>Copy</th>
              
              </tr>
      
              <% mySheetList.forEach(function(mySheet) { %>
                  <tr>
                    <td><%= mySheet.name%></td>
                    <td><%= mySheet.creator%></td>
                    <td> <button type="button" name="CopyButton">Copy</button> </td>
                  
                    
          
                  </tr>
              <% }); %>
  
         </table>
      


    </div>

 
      

    <script>





      </script>
  </body>
</html>
